{
    "env": {
        "node": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "global-require": 1,
        "no-var": 1,
        "no-multiple-empty-lines": [
            1,
            {
                "max": 1,
                "maxBOF": 0,
                "maxEOF": 0
            }
        ],
        "no-console": 0,
        "indent": [
            "error",
            4
        ],
        "quotes": [
            2,
            "double"
        ]
    }
}